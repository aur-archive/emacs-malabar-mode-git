<!--
In the fakeroot enviroment the mvn command always wants to use the
root/.m2/reprository folder. Thats why it is neccesary to use this file to say mvn command to use the /tmp/m2_localRepository folder instead.
 -->

<settings>
  <localRepository>/tmp/m2_localRepository</localRepository>
  <proxies>
   <proxy>
      <active>false</active>
      <protocol>http</protocol>
      <host></host>
      <port></port>
      <username></username>
      <password></password>
    </proxy>
  </proxies>
</settings>
